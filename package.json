{
  "name": "configurator",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npm run asbuild && next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "pages:build": "npm run asbuild && opennextjs-cloudflare build",
    "preview": "npm run asbuild && opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "npm run asbuild && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "asbuild:debug": "asc wasm/assembly/canvas-performance.ts --target debug --outFile public/wasm/canvas-performance.debug.wasm",
    "asbuild:release": "asc wasm/assembly/canvas-performance.ts --target release --outFile public/wasm/canvas-performance.wasm",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.11.0",
    "@supabase/supabase-js": "^2.57.0",
    "@types/uuid": "^10.0.0",
    "jspdf": "^3.0.2",
    "konva": "^9.3.22",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-konva": "^19.0.7",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "assemblyscript": "^0.27.34",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.45.0"
  }
}
